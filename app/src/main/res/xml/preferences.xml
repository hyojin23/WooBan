<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory
        android:key="category"
        android:title="설정"
        android:icon="@drawable/ic_build_gray_24dp"
        />

    <Preference
        android:title="프로필 편집"
        android:key="profile_change"
        app:isPreferenceVisible="false"
        android:icon="@drawable/ic_account_circle_gray_24dp"
        />
    <Preference
        android:title="비밀번호 변경"
        android:key="password_change"
        app:isPreferenceVisible="false"
        android:icon="@drawable/ic_lock_outline_gray_24dp"
        />
    <SwitchPreference
        android:title="푸시 알람 받기"
        android:key="push_alarm"
        app:isPreferenceVisible="false"
        android:icon="@drawable/ic_chat_bubble_outline_gray_24dp"
        />
    <Preference
        android:title="버전 정보"
        android:key="version"
        android:summary="1.0.0"
        android:icon="@drawable/ic_info_outline_gray_24dp"
        />

</PreferenceScreen>